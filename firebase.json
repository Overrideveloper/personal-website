{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|js|otf|css|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    }]
  }
}
